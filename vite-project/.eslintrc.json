{
  "extends": ["@rocketseat/eslint-config/react", "prettier"],
  "plugins": ["prettier", "react", "react-hooks"],
  "prettier/prettier": [
    "error",
    {
      "trailingComma": "es5",
      "singleQuote": true,
      "printWidth": 100,
      "tabWidth": 2,
      "semi": true,
      "endOfLine": "auto"
    }
  ]
}
